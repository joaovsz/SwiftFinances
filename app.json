{
  "expo": {
    "name": "SwiftFinances",
    "slug": "SwiftFinances",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.joaovsz.swiftfinances"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router", ["expo-font"],
        ["expo-camera",
          {
            "cameraPermission": "Permitir SwiftFinances acessar sua c√¢mera?",
            "microphonePermission": "Permitir SwiftFinances acessar seu microfone?",
            "recordAudioAndroid": true
          }
        ],
        [
          "expo-image-picker",
          {
            "permission": "Permitir SwiftFinances acessar sua galeria de fotos?",
            "ios": {
              "NSPhotoLibraryUsageDescription": "Permitir SwiftFinances acessar sua galeria de fotos?"
            }
          }
        ]        
            
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "dc5cba70-92cc-46e8-86a1-2ab64f4bcf2f"
      }
    }
  }
}
